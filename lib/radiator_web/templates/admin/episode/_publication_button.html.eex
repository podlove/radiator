<%= form_for Radiator.Directory.Episode.changeset(@episode, %{}), Routes.admin_network_podcast_episode_path(@conn, :update, @episode.podcast.network_id, @episode.podcast_id, @episode.id), fn f -> %>
  <%= hidden_input f, :publish_state, value: "published" %>
  <input type="submit" value="Publish Episode" class="bg-green-500 hover:bg-green-400 text-white font-bold py-2 px-4 border-b-4 border-green-700 hover:border-green-500 rounded">
<% end %>
